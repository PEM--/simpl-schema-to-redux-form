{
  "name": "simpl-schema-to-redux-form",
  "version": "0.0.1",
  "description": "SimpleSchema to Redux Form",
  "main": "index.js",
  "repository": "https://github.com/PEM--/simpl-schema-to-redux-form.git",
  "author": "PEM-- <pemarchandet@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel --presets latest src -d ./ --ignore src/index.test.js",
    "dev": "babel --presets latest src -d ./ --watch --ignore src/index.test.js",
    "test": "jest",
    "test.watch": "jest --watch --notify",
    "lint": "standard --global it --global expect 'src/*.js' | snazzy",
    "lint.fix": "standard --global it --global expect 'src/*.js' --fix",
    "prepush": "npm run lint && npm test"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2",
    "storybook": "^0.0.0"
  },
  "dependencies": {
    "dot-object": "^1.6.0",
    "simpl-schema": "^0.3.1"
  },
  "standard.options": {
    "globals": [
      "it",
      "expect"
    ],
    "ignore": [
      "node_modules/**"
    ],
    "plugins": [
      "class-property",
      "html"
    ],
    "parser": "babel-eslint"
  }
}
