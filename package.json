{
  "name": "simpl-schema-to-redux-form",
  "version": "1.0.2",
  "description": "SimpleSchema to Redux Form",
  "main": "index.min.js",
  "repository": "https://github.com/PEM--/simpl-schema-to-redux-form.git",
  "author": "PEM-- <pemarchandet@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV='production' rollup -c",
    "dev": "NODE_ENV='development' rollup -c",
    "dev.watch": "NODE_ENV='development' rollup -c --watch",
    "watch": "npm-run-all --parallel dev.watch test.watch",
    "test": "NODE_ENV='testing' jest",
    "test.watch": "NODE_ENV='testing' jest --watch --notify",
    "lint": "standard --global it --global expect 'src/*.js' | snazzy",
    "lint.fix": "standard --global it --global expect 'src/*.js' --fix",
    "prepush": "npm run lint && npm test"
  },
  "devDependencies": {
    "ajv": "^5.2.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.7.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "jest": "^21.1.0",
    "npm-run-all": "^4.1.1",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2",
    "uglify-es": "^3.1.1"
  },
  "dependencies": {
    "dot-object": "^1.6.0",
    "simpl-schema": "^0.3.2"
  },
  "standard.options": {
    "globals": [
      "it",
      "expect"
    ],
    "ignore": [
      "node_modules/**"
    ],
    "plugins": [
      "class-property",
      "html"
    ],
    "parser": "babel-eslint"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  }
}
